<%= form_tag(post_booking_path) do %>
  <%= label_tag 'Tourist email' %>
  <%= text_field_tag :guest %><br>
  <%= label_tag 'Starting Location' %>
  <%= text_field_tag :meet_location %><br>
  <%= label_tag 'Start Time' %>
  <%= text_field_tag :start_time %><br>
  <%= label_tag 'End Time' %>
  <%= text_field_tag :end_time %><br>
  <%= label_tag 'Summary' %>
  <%= text_area_tag :summary %><br>
  <%= submit_tag %>
<% end %>
